@{
    Session["diagnosisKeyword"] = "diagnosis-q4_a";
}
@Html.Hidden("pageKey", Session["diagnosisKeyword"])
<div class="main-diagnosis">
    <img src="~/Content/images/pc/diagnosis/1.jpg" />
    <div class="content-diagnosis qa">
        <div class="check">Check 4</div>
        <div class="question"><h1>プチ贅沢ヘアケアを試すなら？</h1></div>
        <div class="choices big">
            <a href="/diagnosis/q5/a2" class="box">
                <div>
                    <span class="title">A</span><br>
                    <p>トリートメントに<br>まぜるオイルで、<br>新感覚ケア</p>
                    <img src="~/Content/images/pc/diagnosis/arrow.png" />
                </div>
            </a>
            <a href="/diagnosis/q5/b2" class="box">
                <div>
                    <span class="title">B</span><br>
                    <p>ヘアマスクや洗い流<br>すトリートメントで、<br>ディープケア</p>
                    <img src="~/Content/images/pc/diagnosis/arrow.png" />
                </div>
            </a>
        </div>
        <div class="steps">
            <img src="/Content/images/pc/diagnosis/step4.png">
            <div class="desc"><a href="/diagnosis/q3/a2">< ひとつ前にもどる</a></div>
        </div>
    </div>
</div>